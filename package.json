{
  "name": "spanishdex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- cross-env NODE_OPTIONS=\"--loader newrelic/esm-loader.mjs --require newrelic\" next dev --turbopack",
    "build": "next build",
    "start": "dotenv -e .env.local -- cross-env NODE_OPTIONS=\"-r newrelic\" next start",
    "lint": "next lint"
  },
  "packageManager": "npm@10.7.0",
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "next": "$next"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "@vercel/blob": "^0.27.2",
    "@vercel/speed-insights": "^1.2.0",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "generate-password": "^1.7.1",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^6.13.0",
    "newrelic": "^12.19.0",
    "next": "^15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "react": "19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "19.0.0",
    "react-easy-crop": "^5.4.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "sass": "1.77.6"
  }
}
